	position:relative;
	background: rgb(81,3,3); /* Old browsers */
	background: -moz-linear-gradient(left, rgba(81,3,3,1) 0%, rgba(173,8,8,1) 50%, rgba(61,0,0,1) 100%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, right top, color-stop(0%,rgba(81,3,3,1)), color-stop(50%,rgba(173,8,8,1)), color-stop(100%,rgba(61,0,0,1))); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(left, rgba(81,3,3,1) 0%,rgba(173,8,8,1) 50%,rgba(61,0,0,1) 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(left, rgba(81,3,3,1) 0%,rgba(173,8,8,1) 50%,rgba(61,0,0,1) 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(left, rgba(81,3,3,1) 0%,rgba(173,8,8,1) 50%,rgba(61,0,0,1) 100%); /* IE10+ */
	background: linear-gradient(to right, rgba(81,3,3,1) 0%,rgba(173,8,8,1) 50%,rgba(61,0,0,1) 100%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#510303', endColorstr='#3d0000',GradientType=1 ); /* IE6-9 */
	box-shadow:0 0 50px rgba(0,0,0,0.5);
}
