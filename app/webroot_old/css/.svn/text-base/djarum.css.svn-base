.merchandiseBox .points{
	font-family: 'Doppio One', sans-serif;
	width:80px;
	background: #6d5f48; /* Old browsers */
	/* IE9 SVG, needs conditional override of 'filter' to 'none' */
	background: url(data:image/svg+xml;base64,);
	background: -moz-linear-gradient(top, #6d5f48 0%, #8d7d61 58%, #b5a78b 100%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#6d5f48), color-stop(58%,#8d7d61), color-stop(100%,#b5a78b)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top, #6d5f48 0%,#8d7d61 58%,#b5a78b 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top, #6d5f48 0%,#8d7d61 58%,#b5a78b 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top, #6d5f48 0%,#8d7d61 58%,#b5a78b 100%); /* IE10+ */
	background: linear-gradient(to bottom, #6d5f48 0%,#8d7d61 58%,#b5a78b 100%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#6d5f48', endColorstr='#b5a78b',GradientType=0 );
	box-shadow:0  -1px 5px #453a25 inset;
	line-height:10px;
	position:absolute;
	color:#fff;
	height:40px;
	text-align:center;
	font-size:16px;
	text-shadow:0 0 1px #000;
}
