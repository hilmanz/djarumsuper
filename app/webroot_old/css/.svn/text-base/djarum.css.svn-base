}
.picBox{
	background: #afa699; /* Old browsers */
	/* IE9 SVG, needs conditional override of 'filter' to 'none' */
	background: url(data:image/svg+xml;base64,);
	background: -moz-linear-gradient(top, #afa699 0%, #7b6e5b 100%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#afa699), color-stop(100%,#7b6e5b)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top, #afa699 0%,#7b6e5b 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top, #afa699 0%,#7b6e5b 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top, #afa699 0%,#7b6e5b 100%); /* IE10+ */
	background: linear-gradient(to bottom, #afa699 0%,#7b6e5b 100%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#afa699', endColorstr='#7b6e5b',GradientType=0 );
	padding:8px;
	width:150px;
	height:95px;
	overflow:hidden;
	float:left;
	box-shadow:2px 2px 4px #3f3424;
	margin:8px;
}
.picBox a{
	display:block;
	width:150px;
	height:95px;
	overflow:hidden;
	background:#000;
}
.picBox a:hover img{
	opacity:1;
	-webkit-transition: all 0.3s ease-in-out;
	-moz-transition: all 0.3s ease-in-out;
	-o-transition: all 0.3s ease-in-out;
	-ms-transition: all 0.3s ease-in-out;
	transition: all 0.3s ease-in-out;
}
.picBox img{
	width:150px;
	height:95px;
	box-shadow:2px 2px 4px #3f3424 inset;
	opacity:0.6;
}
